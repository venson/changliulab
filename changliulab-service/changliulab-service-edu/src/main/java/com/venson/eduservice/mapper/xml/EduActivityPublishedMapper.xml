<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.venson.eduservice.mapper.EduActivityPublishedMapper">

    <select id="getFrontIndexActivity"
            resultType="com.venson.eduservice.entity.front.dto.ActivityFrontBriefDTO">
        select id, title, activity_date
            from edu_activity_published
        order by id
    </select>
</mapper>
